{
  "name": "check-codeowners",
  "description": "get stats about a repositories CODEOWNERS file",
  "version": "0.0.0",
  "author": "Marco Link @marcolink",
  "bin": {
    "check-codeowners": "./bin/run"
  },
  "bugs": "https://github.com/marcolink/check-codeowners/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "chalk": "^4.0.0",
    "fs-extra": "^9.0.0",
    "micromatch": "^4.0.2",
    "recursive-readdir": "^2.2.2",
    "table": "^5.4.6",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@semantic-release/changelog": "^5.0.1",
    "@types/chai": "^4",
    "@types/fs-extra": "^8.1.0",
    "@types/micromatch": "^4.0.1",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/recursive-readdir": "^2.2.0",
    "@types/table": "^5.0.0",
    "chai": "^4",
    "mocha": "^5",
    "nyc": "^14",
    "semantic-release": "^19.0.3",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/marcolink/check-codeowners",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "check-codeowners"
  },
  "repository": "marcolink/check-codeowners",
  "scripts": {
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "build": "tsc -b",
    "presemantic-release": "npm run build",
    "semantic-release": "semantic-release"
  },
  "types": "lib/index.d.ts",
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "build",
              "scope": "deps",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/changelog",
      "@semantic-release/github"
    ]
  }
}
